@require '_lib'

@supports (font-variation-settings: normal)
  // Public Sans
  @font-face
    font-weight: 100 900
    font-family: 'Public Sans'
    src: url('/static/fonts/variable/PublicSans-VariableFont.woff2') format('woff2-variations')
    font-display: swap

  @font-face
    font-weight: 100 900
    font-family: 'Public Sans'
    font-style: italic
    src: url('/static/fonts/variable/PublicSans-Italic-VariableFont.woff2') format('woff2-variations')
    font-display: swap

  // Bitter
  @font-face
    font-weight: 100 900
    font-family: 'Bitter'
    src: url('/static/fonts/variable/Bitter-VariableFont.woff2') format('woff2-variations')
    font-display: swap

  @font-face
