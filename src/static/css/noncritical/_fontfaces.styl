// Variable fonts
@supports (font-variation-settings: normal)
  // Vollkorn v4.106
  @font-face
    font-family: 'Vollkorn'
    src: url('/static/fonts/variable/Vollkorn-VariableFont.woff2') format('woff2')
    font-weight: 400 900
    font-display: swap

  @font-face
    font-family: 'Vollkorn'
    src: url('/static/fonts/variable/Vollkorn-Italic-VariableFont.woff2') format('woff2')
    font-weight: 400 900
    font-style: italic
    font-display: swap

  // Inter v3.15
  @font-face
    font-family: 'Inter'
    src: url('/static/fonts/variable/Inter-VariableFont.woff2') format('woff2')
    font-weight: 100 900
    font-display: swap
  
  // Fira Code v5.2
  @font-face
    font-family: 'Fira Code'
    src: url('/static/fonts/variable/FiraCode-VF.woff2') format('woff2')
    font-weight: 300 700
    font-display: swap
  
